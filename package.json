{
  "name": "dashboard",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "serve": "cross-env NODE_ENV=development && firebase emulators:start --only=hosting,functions,firestore,storage,auth --import .data --export-on-exit --project=demo-tailed1",
    "build": "tsc --noEmitOnError false --skipLibCheck --allowJs --checkJs false --noImplicitAny false --strict false --strictNullChecks false && vite build && cd functions && npm run build && cd ..",
    "lint": "eslint .",
    "preview": "vite preview",
    "machine-translate": "inlang machine translate --project project.inlang",
    "inlang-compile": "npx @inlang/paraglide-js compile --project ./project.inlang --outdir ./src/paraglide",
    "prepare": "husky"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.1.1",
    "@tailwindcss/vite": "^4.1.10",
    "@tiptap/extension-placeholder": "^3.14.0",
    "@tiptap/react": "^3.14.0",
    "@tiptap/starter-kit": "^3.14.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "firebase": "^11.10.0",
    "lucide-react": "^0.523.0",
    "next-themes": "^0.4.6",
    "radix-ui": "^1.4.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.58.1",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.6.3",
    "sonner": "^2.0.5",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.10",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@inlang/cli": "^3.0.0",
    "@inlang/paraglide-js": "2.1.0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.5.2",
    "commitizen": "^4.3.1",
    "cross-env": "^10.0.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.29.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.2.0",
    "husky": "^9.1.7",
    "tw-animate-css": "^1.3.4",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.34.1",
    "vite": "^6.3.5"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
