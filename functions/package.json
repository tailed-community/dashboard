{
  "name": "functions",
  "type": "commonjs",
  "scripts": {
    "lint-backup": "eslint --ext .js,.ts .",
    "lint": "",
    "build-backup": "tsc",
    "build": "tsc --noEmitOnError false --skipLibCheck --allowJs --checkJs false --noImplicitAny false --strict false --strictNullChecks false --suppressImplicitAnyIndexErrors",
    "build:watch": "tsc --watch",
    "dev": "nodemon src/index.ts",
    "serve": "cd .. && cross-env NODE_ENV=development firebase emulators:start --only functions --import .data --export-on-exit",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log"
  },
  "engines": {
    "node": "22"
  },
  "main": "lib/index.js",
  "dependencies": {
    "@firebase/app": "^0.11.2",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "csv-parse": "^5.5.4",
    "dotenv": "^16.4.7",
    "express": "^5.1.0",
    "firebase": "^11.5.0",
    "firebase-admin": "^12.6.0",
    "firebase-functions": "^6.0.1",
    "multer": "^2.0.2",
    "nodemailer": "^6.10.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.8",
    "@types/express": "^5.0.3",
    "@types/multer": "^2.0.0",
    "@types/nodemailer": "^6.4.17",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "cross-env": "^10.0.0",
    "eslint": "^9.24.0",
    "eslint-plugin-import": "^2.31.0",
    "firebase-functions-test": "^3.1.0",
    "husky": "^9.1.7",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "typescript": "^4.9.5"
  },
  "private": true
}
