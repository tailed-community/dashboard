# Firebase Configuration for Community Functions
# Copy this to .env and update with your values

# Firebase Project Configuration
FB_API_KEY=your_api_key
FB_AUTH_DOMAIN=your_project_id.firebaseapp.com
FB_PROJECT_ID=your_project_id
FB_STORAGE_BUCKET=your_project_id.appspot.com
FB_MESSAGING_SENDER_ID=your_messaging_sender_id
FB_APP_ID=your_app_id

# Environment
NODE_ENV=development

# Tenant IDs (if using multi-tenancy)
STUDENT_TENANT_ID=

# For local development with emulators, use:
# FB_PROJECT_ID=demo-tailed1
# FB_STORAGE_BUCKET=demo-tailed1.appspot.com

